<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TitledPane?>


<TitledPane fx:id="root" expanded="false" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gamergeo.project.videomanager.view.table.TableView">
   <content>
      <TableView fx:id="table" prefHeight="300.0">
      	
      	<columns>
      	    <TableColumn fx:id="titleColumn" prefWidth="300" text="Title" />
      	    <TableColumn fx:id="ratingColumn" prefWidth="40" text="Rating" />
      	    <TableColumn fx:id="tagsColumn" prefWidth="300" text="Tags" />
      	</columns>
      	 <columnResizePolicy>
      	    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
      	 </columnResizePolicy>
      </TableView>
   </content>
</TitledPane>
