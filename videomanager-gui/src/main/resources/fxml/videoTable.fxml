<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE project>

<?import java.lang.*?>
<?import javafx.scene.control.*?>

<TableView fx:id="videoTable" onMousePressed="#selectVideo" fx:controller="com.gamergeo.project.videomanager.gui.controller.VideoTableController"
	prefHeight="145.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" >
	
	<columns>
	    <TableColumn fx:id="titleColumn" prefWidth="200" text="Title" />
	    <TableColumn fx:id="tagsColumn" prefWidth="400" text="Tags" />
	</columns>
	 <columnResizePolicy>
	    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
	 </columnResizePolicy>
</TableView>
